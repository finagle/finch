{
    "basePath": "/",
    "definitions": {
        "ApiResponse": {
            "properties": {
                "code": {
                    "format": "int32",
                    "type": "integer"
                },
                "message": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "Category": {
            "properties": {
                "id": {
                    "format": "int64",
                    "type": "long"
                },
                "name": {
                    "type": "string"
                }
            },
            "required": ["name"],
            "type": "object",
            "xml": {
                "name": "Category"
            }
        },
        "Order": {
            "properties": {
                "id": {
                    "format": "int64",
                    "type": "long",
                    "example": ""
                },
                "petId": {
                    "format": "int64",
                    "type": "integer",
                    "example": 2
                },
                "quantity": {
                    "format": "int32",
                    "type": "integer",
                    "example": 10
                },
                "shipDate": {
                    "format": "date-time",
                    "type": "string"
                },
                "status": {
                    "description": "Order Status",
                    "enum": [
                        "placed",
                        "approved",
                        "delivered"
                    ],
                    "type": "string"
                },
                "complete": {
                  "default": false,
                  "type": "boolean"
                }
            },
            "type": "object",
            "xml": {
                "name": "Order"
            }
        },
        "Pet": {
            "properties": {
                "id": {
                    "format": "int64",
                    "type": "long",
                    "example": ""
                },
                "name": {
                    "type": "string",
                    "example": "Coco"
                },
                "photoUrls": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array",
                    "example": ["url_to_coco_pic"],
                    "xml": {
                        "name": "photoUrl",
                        "wrapped": true
                    }
                },
                "category": {
                  "$ref": "#/definitions/Category",
                  "example": {
                    "name": "Cat"
                  }
                },
                "tags": {
                    "items": {
                        "$ref": "#/definitions/Tag"
                    },
                    "type": "array",
                    "xml": {
                        "name": "tag",
                        "wrapped": true
                    },
                    "example": [
                      {"name": "white"},
                      {"name": "persian"}
                    ]
                },
                "status": {
                  "description": "pet status in the store",
                  "enum": [
                    "available",
                    "pending",
                    "adopted"
                  ],
                  "type": "string"
                }
            },
            "required": [
                "name",
                "photoUrls"
            ],
            "type": "object",
            "xml": {
                "name": "Pet"
            }
        },
        "Tag": {
            "properties": {
                "id": {
                    "format": "int64",
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            },
            "required": ["name"],
            "type": "object",
            "xml": {
                "name": "Tag"
            }
        },
        "User": {
            "properties": {
                "id": {
                  "format": "int64",
                  "type": "long",
                  "example": ""
                },
                "username": {
                  "type": "string",
                  "example": "coraline"
                },
                "firstName": {
                    "type": "string",
                    "example": "Coraline"
                },
                "lastName": {
                    "type": "string",
                    "example": "Jones"
                },
                "email": {
                  "type": "string",
                  "example": "yellow_raincoat@gmail.com"
                },
                "password": {
                    "type": "string",
                    "example": "blackcat"
                },
                "phone": {
                    "type": "string",
                    "example": "408-000-0000"
                }
            },
            "type": "object",
            "required": ["username", "password"],
            "xml": {
                "name": "User"
            }
        },
      "UpdatePetInputBody": {
        "properties": {
          "id": {
            "format": "int64",
            "type": "long"
          },
          "name": {
            "type": "string",
            "required": true
          },
          "photoUrls": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "xml": {
              "name": "photoUrl",
              "wrapped": true
            }
          },
          "category": {
            "$ref": "#/definitions/InputCat"
          },
          "tags": {
            "items": {
              "$ref": "#/definitions/InputTag"
            },
            "type": "array",
            "xml": {
              "name": "tag",
              "wrapped": true
            }
          },
          "status": {
            "description": "pet status in the store",
            "enum": [
              "available",
              "pending",
              "adopted"
            ],
            "type": "string"
          }
        },
        "type": "object",
        "required": ["id", "name", "photoUrls"],
        "example": {
          "id": 0,
          "name": "Rumpleteazer",
          "photoUrls": ["someUrl"],
          "category": {
            "name": "Cat"
          },
          "tags": [
            {
              "name": "orange"
            },
            {
              "name": "tabby"
            }
          ],
          "status": "available"
        },
        "xml": {
          "name": "Pet"
        }
      }
    },
    "externalDocs": {
        "description": "Find out more about Finch:",
        "url": "https://github.com/finagle/finch"
    },
    "host": "localhost:8080",
    "info": {
        "contact": {
            "gittr": "https://gitter.im/finagle/finch"
        },
        "description": "This is a sample server Petstore server built using Finch. This API is based off the Swagger petstore demo (http://petstore.swagger.io).",
        "title": "Finch Petstore",
        "version": "1.0.0"
    },
    "paths": {
        "/pet": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "description": "Allows you to add a new pet to the petstore! Note: Any body passed in as a potential pet should not have an id. Neither should Category or Tag objects. Ids should be auto-generated only!",
                "operationId": "addPet",
                "parameters": [
                  {
                    "description": "Pet object that needs to be added to the store",
                    "in": "body",
                    "name": "body",
                    "required": true,
                    "schema": {
                      "$ref": "#/definitions/Pet"
                    }
                  }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "404": {
                        "description": "Invalid input"
                    },
                    "400": {
                        "description": "Bad input: You probably omitted a required field"
                    }
                },
                "summary": "Add a new pet to the store",
                "tags": [
                    "pet"
                ]
            },
            "put": {
                "consumes": [
                    "application/json"
                ],
                "description": "Allows you to update existing pets in the store! Bodies given as input must include an ID so the API knows which pet to update. As with addPet, any added categories or tags should not have IDs.",
                "operationId": "updatePet",
                "parameters": [
                    {
                        "description": "Pet object that needs to be added to the store",
                        "in": "body",
                        "name": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/Pet"
                        }
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "404": {
                        "description": "The pet to be updated cannot be found"
                    }
                },
                "summary": "Update an existing pet",
                "tags": [
                    "pet"
                ]
            }
        },
        "/pet/findByStatus": {
            "get": {
                "description": "In a real use-case, multiple status values can be provided with comma-separated strings. If you're using the UI to interact with the path, simply holding down command (on Mac) will allow you to select more than one status to search for.",
                "operationId": "findPetsByStatus",
                "parameters": [
                    {
                        "collectionFormat": "csv",
                        "description": "Status values that need to be considered for filter",
                        "in": "query",
                        "items": {
                            "default": "available",
                            "enum": [
                                "available",
                                "pending",
                                "sold"
                            ],
                            "type": "string"
                        },
                        "name": "status",
                        "required": true,
                        "type": "array"
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "items": {
                                "$ref": "#/definitions/Pet"
                            },
                            "type": "array"
                        }
                    },
                    "400": {
                        "description": "Invalid status value"
                    }
                },
                "summary": "Finds Pets by status",
                "tags": [
                    "pet"
                ]
            }
        },
        "/pet/findByTags": {
            "get": {
                "description": "Muliple tags can be provided with comma-seperated strings. Use 'white' and 'fluffy' for testing.",
                "operationId": "findPetsByTags",
                "parameters": [
                    {
                        "collectionFormat": "csv",
                        "description": "Tags to filter by",
                        "in": "query",
                        "items": {
                            "type": "string"
                        },
                        "name": "tags",
                        "required": true,
                        "type": "array"
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "items": {
                                "$ref": "#/definitions/Pet"
                            },
                            "type": "array"
                        }
                    },
                    "404": {
                        "description": "Invalid tag value"
                    }
                },
                "security": [
                    {
                        "petstore_auth": [
                            "write:pets",
                            "read:pets"
                        ]
                    }
                ],
                "summary": "Finds Pets by tags",
                "tags": [
                    "pet"
                ]
            }
        },
        "/pet/{petId}": {
            "delete": {
                "description": "",
                "operationId": "deletePet",
                "parameters": [
                    {
                        "description": "Pet id to delete",
                        "format": "int64",
                        "in": "path",
                        "name": "petId",
                        "required": true,
                        "type": "long"
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "400": {
                        "description": "Invalid pet value"
                    }
                },
                "summary": "Deletes a pet",
                "tags": [
                    "pet"
                ]
            },
            "get": {
                "description": "Returns a single pet",
                "operationId": "getPetById",
                "parameters": [
                    {
                        "description": "ID of pet to return",
                        "format": "int64",
                        "in": "path",
                        "name": "petId",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "$ref": "#/definitions/Pet"
                        }
                    },
                    "404": {
                        "description": "Pet not found / invalid ID supplied"
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ],
                "summary": "Find pet by ID",
                "tags": [
                    "pet"
                ]
            },
            "post": {
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "description": "",
                "operationId": "updatePetWithForm",
                "parameters": [
                    {
                        "description": "ID of pet that needs to be updated",
                        "format": "int64",
                        "in": "path",
                        "name": "petId",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "description": "Updated name of the pet",
                        "in": "formData",
                        "name": "name",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "description": "Updated status of the pet",
                        "in": "formData",
                        "name": "status",
                        "required": false,
                        "type": "string"
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "400": {
                      "description": "Invalid input!"
                    },
                    "404": {
                        "description": "The pet to be updated does not exist!"
                    }
                },
                "summary": "Updates a pet in the store with form data",
                "tags": [
                    "pet"
                ]
            }
        },
        "/pet/{petId}/uploadImage": {
            "post": {
                "consumes": [
                    "multipart/form-data"
                ],
                "description": "",
                "operationId": "uploadFile",
                "parameters": [
                    {
                        "description": "ID of pet to update",
                        "format": "int64",
                        "in": "path",
                        "name": "petId",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "description": "Additional data to pass to server",
                        "in": "formData",
                        "name": "additionalMetadata",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "description": "file to upload",
                        "in": "formData",
                        "name": "file",
                        "required": false,
                        "type": "file"
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "$ref": "#/definitions/ApiResponse"
                        }
                    }
                },
                "security": [
                    {
                        "petstore_auth": [
                            "write:pets",
                            "read:pets"
                        ]
                    }
                ],
                "summary": "uploads an image",
                "tags": [
                    "pet"
                ]
            }
        },
        "/store/inventory": {
            "get": {
                "description": "Returns a map of status codes to quantities",
                "operationId": "getInventory",
                "parameters": [],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "additionalProperties": {
                                "format": "int32",
                                "type": "integer"
                            },
                            "type": "object"
                        }
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ],
                "summary": "Returns pet inventories by status",
                "tags": [
                    "store"
                ]
            }
        },
        "/store/order": {
            "post": {
                "description": "",
                "operationId": "placeOrder",
                "parameters": [
                    {
                        "description": "order placed for purchasing the pet",
                        "in": "body",
                        "name": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/Order"
                        }
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "$ref": "#/definitions/Order"
                        }
                    },
                    "400": {
                        "description": "Invalid Order"
                    }
                },
                "summary": "Place an order for a pet",
                "tags": [
                    "store"
                ]
            }
        },
        "/store/order/{orderId}": {
            "delete": {
                "description": "For valid response try integer IDs with value < 1000. Anything above 1000 or nonintegers will generate API errors",
                "operationId": "deleteOrder",
                "parameters": [
                    {
                        "description": "ID of the order that needs to be deleted",
                        "in": "path",
                        "minimum": 1.0,
                        "name": "orderId",
                        "required": true,
                        "type": "string"
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Response body is 'true' if the order was successfully deleted. It is 'false' if the order does not exist."
                    },
                    "404": {
                        "description": "Invalid ID supplied"
                    }
                },
                "summary": "Delete purchase order by ID",
                "tags": [
                    "store"
                ]
            },
            "get": {
                "description": "For valid response try integer IDs with value <= 5 or > 10. Other values will generated exceptions",
                "operationId": "getOrderById",
                "parameters": [
                    {
                        "description": "ID of pet that needs to be fetched",
                        "format": "int64",
                        "in": "path",
                        "maximum": 5.0,
                        "minimum": 1.0,
                        "name": "orderId",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "produces": [
                    "application/xml",
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "$ref": "#/definitions/Order"
                        }
                    },
                    "404": {
                        "description": "Order not found (a message will be printed) or an invalid ID was supplied."
                    }
                },
                "summary": "Find purchase order by ID",
                "tags": [
                    "store"
                ]
            }
        },
        "/user": {
            "post": {
                "description": "This can only be done by the logged in user.",
                "operationId": "createUser",
                "parameters": [
                    {
                        "description": "Created user object",
                        "in": "body",
                        "name": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/User"
                        }
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "default": {
                        "description": "successful operation"
                    }
                },
                "summary": "Create user",
                "tags": [
                    "user"
                ]
            }
        },
        "/user/createWithArray": {
            "post": {
                "description": "",
                "operationId": "createUsersWithArrayInput",
                "parameters": [
                    {
                        "description": "List of user object",
                        "in": "body",
                        "name": "body",
                        "required": true,
                        "schema": {
                            "items": {
                                "$ref": "#/definitions/User"
                            },
                            "type": "array"
                        }
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "default": {
                        "description": "successful operation"
                    }
                },
                "summary": "Creates list of users with given input array",
                "tags": [
                    "user"
                ]
            }
        },
        "/user/createWithList": {
            "post": {
                "description": "",
                "operationId": "createUsersWithListInput",
                "parameters": [
                    {
                        "description": "List of user object",
                        "in": "body",
                        "name": "body",
                        "required": true,
                        "schema": {
                            "items": {
                                "$ref": "#/definitions/User"
                            },
                            "type": "array"
                        }
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "default": {
                        "description": "successful operation"
                    }
                },
                "summary": "Creates list of users with given input array",
                "tags": [
                    "user"
                ]
            }
        },
        "/user/{username}": {
            "delete": {
                "description": "This can only be done by the logged in user.",
                "operationId": "deleteUser",
                "parameters": [
                    {
                        "description": "The name that needs to be deleted",
                        "in": "path",
                        "name": "username",
                        "required": true,
                        "type": "string"
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "404": {
                        "description": "User not found"
                    }
                },
                "summary": "Delete user",
                "tags": [
                    "user"
                ]
            },
            "get": {
                "description": "",
                "operationId": "getUserByName",
                "parameters": [
                    {
                        "description": "The name that needs to be fetched. Use user1 for testing. ",
                        "in": "path",
                        "name": "username",
                        "required": true,
                        "type": "string"
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "$ref": "#/definitions/User"
                        }
                    },
                    "404": {
                        "description": "User not found"
                    }
                },
                "summary": "Get user by user name",
                "tags": [
                    "user"
                ]
            },
            "put": {
                "description": "This must be passed with the exact username of the user you want to update! Usernames cannot change.",
                "operationId": "updateUser",
                "parameters": [
                    {
                        "description": "name that need to be deleted",
                        "in": "path",
                        "name": "username",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "description": "Updated user object",
                        "in": "body",
                        "name": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/User"
                        }
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "404": {
                        "description": "User not found"
                    }
                },
                "summary": "Updated user",
                "tags": [
                    "user"
                ]
            }
        }
    },
    "schemes": [
        "http"
    ],
    "securityDefinitions": {
        "api_key": {
            "in": "header",
            "name": "api_key",
            "type": "apiKey"
        },
        "petstore_auth": {
            "authorizationUrl": "http://petstore.swagger.io/api/oauth/dialog",
            "flow": "implicit",
            "scopes": {
                "read:pets": "read your pets",
                "write:pets": "modify pets in your account"
            },
            "type": "oauth2"
        }
    },
    "swagger": "2.0",
    "tags": [
        {
            "description": "Everything about your Pets",
            "externalDocs": {
                "description": "Find out more",
                "url": "http://swagger.io"
            },
            "name": "pet"
        },
        {
            "description": "Access to Petstore orders",
            "name": "store"
        },
        {
            "description": "Operations about user",
            "externalDocs": {
                "description": "Find out more about our store",
                "url": "http://swagger.io"
            },
            "name": "user"
        }
    ]
}
